<div ng-controller="cellarListCtrl">
<h1>{{pageHeader.username}}'s {{pageHeader.title}}</h1>
  <div class="container-fluid">
    <div class="row">
      <!-- <div class="col-md-8 col-xs-12"> -->
        <div id='cellar-list-container'>
          <label id="beer-search-label" for="filter">Search</label>
          <input id="beer-search" type="text" class="form-control" name="filter" ng-model="textFilter">
          <table class="table table-striped">
            <tr>
              <th>Brewery</th>
              <th>Beer</th>
              <th>Style</th>
              <th>Bottling Date</th>
              <th>Qty</th>
              <th>For Trade?</th>
            </tr>

            <tr class="beer-row" ng-repeat="beer in data.beers | filter : textFilter">
              <td>//beer.brewery//</td>
              <td>//beer.beer//</td>
              <td>//beer.style//</td>
              <td>//beer.date//</td>
              <td>//beer.quantity//</td>
              <td>//beer.forTrade//</td>
            </tr>



          </table>
         </div>
        </div>

        <div class="col-md-2 col-xs-8">
      </div>
    </div>
  </div>

   <div id="userId">{{theId}}</div>
